datasource chinook {
  provider = "sqlite"
  url = "file:./db/NestedDeleteMutationInsideUpdateSpec.db"
}

model Parent {
    p             String    @unique
    p_1           String?
    p_2           String?
    childOpt      Child?
    non_unique    String?
    id            String    @id @default(cuid())

    @@unique([p_1, p_2])
}

model Child {
    c             String    @unique
    c_1           String?
    c_2           String?
    parentOpt     Parent?          @relation(references: [p])
    non_unique    String?
    id            String    @id @default(cuid())

    @@unique([c_1, c_2])
}