datasource chinook {
  provider = "mysql"
  url      = "mysql://root:prisma@127.0.0.1:3306/NestedUpsertMutationInsideUpdateSpec"
}

generator js {
  provider        = "prisma-client-js"
  previewFeatures = ["microsoftSqlServer", "mongodb", "orderByRelation", "napi", "selectRelationCount", "orderByAggregateGroup"]
}

model Parent {
  p           String  @unique
  p_1         String
  p_2         String
  childrenOpt Child[]
  non_unique  String?
  id          String  @id @default(cuid())

  @@unique([p_1, p_2])
}

model Child {
  c          String  @unique
  c_1        String
  c_2        String
  parentReq  Parent  @relation(fields: [parentId], references: [id])
  parentId   String
  non_unique String?


  @@unique([c_1, c_2])
}
