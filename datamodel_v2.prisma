datasource test {
  provider = "postgres"
  url      = "postgresql://postgres:prisma@localhost:5432/db?schema=JsonListsSpec&connection_limit=10"
}

generator js {
  provider        = "prisma-client-js"
  previewFeatures = ["nativeTypes"]
}

model ScalarModel {
  id    Int     @id
  gql   String?
  json  Json[]  @test.Json
  jsonB Json[]  @test.JsonB
}
